<script>
function handleSubmit(event) {
  event.preventDefault(); // Prevent default form submission
  var form = document.getElementById('custom-form');
  
  // Use Fetch API to send form data to Google Apps Script
  fetch(form.action, {
    method: 'POST',
    mode: 'no-cors', // This mode bypasses CORS
    body: new FormData(form),
  }).then(() => {
    // Show thank-you popup
    alert('Thank you for your submission!'); // You can customize this popup

    // Redirect after a short delay (e.g., 2 seconds)
    setTimeout(() => {
      window.location.href = 'pages/thank-you'; // Redirect to thank-you page after delay
    }, 2000); // Change delay as needed (2000 ms = 2 seconds)
  }).catch(error => {
    console.error('Error:', error);
    alert('There was an error submitting the form.');
  });
}

</script> 
 
<script>
  function handleSubmit(event) {
    event.preventDefault(); // Prevent form from submitting normally

    const form = document.getElementById('custom-form');
    
    // Form submission with fetch API
    fetch(form.action, {
      method: 'POST',
      mode: 'no-cors', // Bypass CORS
      body: new FormData(form)
    })
    .then(() => {
      // Redirect after submission
      window.location.href = '/pages/thank-you'; // Redirect to thank-you page
    })
    .catch(error => {
      console.error('Error:', error);
      alert('There was an error submitting the form.');
    });
  }

  document.getElementById('custom-form').addEventListener('submit', handleSubmit);
</script>
